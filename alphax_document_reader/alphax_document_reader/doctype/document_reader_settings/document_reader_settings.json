{
  "doctype": "DocType",
  "name": "Document Reader Settings",
  "module": "AlphaX Document Reader",
  "custom": 0,
  "istable": 0,
  "issingle": 1,
  "is_submittable": 0,
  "autoname": "prompt",
  "fields": [
    {
      "fieldname": "enabled",
      "fieldtype": "Check",
      "label": "Enabled",
      "default": "1"
    },
    {
      "fieldname": "allow_ocr",
      "fieldtype": "Check",
      "label": "Allow OCR (if installed)",
      "default": "0"
    },
    {
      "fieldname": "max_pages",
      "fieldtype": "Int",
      "label": "Max PDF Pages",
      "default": "50"
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "submit": 0,
      "cancel": 0,
      "amend": 0
    }
  ]
}